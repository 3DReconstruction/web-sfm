<div class="welcome-screen__detail__toggle" {{action "close"}}></div>

<div class="welcome-screen__detail__progress">

    <ul class="welcome-screen__detail__download-selections">

        <li>

            <h1>
                Images {{#if project.imagesFinished}}loaded{{else}}avaliable{{/if}}
            </h1>

        </li>

        <li class="{{if project.hasFeature '' 'not-avaliable'}}">

            <h1>
                Features {{#if project.hasFeature}}avaliable{{else}}not avaliable{{/if}}
            </h1>

        </li>

        <li class="{{if project.hasMatch '' 'not-avaliable'}}">

            <h1>
                Matches {{#if project.hasMatch}}avaliable{{else}}not avaliable{{/if}}
            </h1>

        </li>

        <li class="{{if project.hasCalibration '' 'not-avaliable'}}">

            <h1>
                Calibration {{#if project.hasCalibration}}avaliable{{else}}not avaliable{{/if}}
            </h1>

        </li>

        <li class="{{if project.hasMVS '' 'not-avaliable'}}">

            <h1>
                MVS {{#if project.hasMVS}}avaliable{{else}}not avaliable{{/if}}
            </h1>

        </li>

    </ul>

    <div class="welcome-screen__detail__download" {{action "enter"}}>
        <h1>Enter</h1>
    </div>

</div>